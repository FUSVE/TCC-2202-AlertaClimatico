{
  "expo": {
    "name": "alerta-climatico-app",
    "slug": "alerta-climatico-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../../assets/logotipo.png",
    "userInterfaceStyle": "light",
